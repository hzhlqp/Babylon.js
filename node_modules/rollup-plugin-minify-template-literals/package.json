  "license": "MIT",
  "author": "Henry Gressmann <mail@henrygressmann.de> (https://henrygressmann.de)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.cjs",
